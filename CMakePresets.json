{
  "version": 8,
  "configurePresets": [
    {
      "name": "GCC 15.2.0 aarch64-apple-darwin23",
      "displayName": "GCC 15.2.0 aarch64-apple-darwin23",
      "description": "Using compilers: C = /opt/homebrew/bin/aarch64-apple-darwin23-gcc-15, CXX = /opt/homebrew/bin/aarch64-apple-darwin23-g++-15",
      "binaryDir": "${sourceDir}/out/build/${presetName}",
      "cacheVariables": {
        "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
        "CMAKE_C_COMPILER": "/opt/homebrew/bin/aarch64-apple-darwin23-gcc-15",
        "CMAKE_CXX_COMPILER": "/opt/homebrew/bin/aarch64-apple-darwin23-g++-15",
        "CMAKE_BUILD_TYPE": "Debug"
      }
    }
  ]
}